{
  "name": "hackerlog-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://hackerlog.io",
  "scripts": {
    "analyze": "ANALYZE=1 react-app-rewired build",
    "build": "react-app-rewired build",
    "build:api": "cd src/services/api/src && npm run build",
    "build-netlify": "REACT_APP_BRANCH=\"$BRANCH\" REACT_APP_COMMIT_REF=\"$COMMIT_REF\" REACT_APP_CONTEXT=\"$CONTEXT\" npm run build",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "postinstall": "cd src/services/api/src && npm install",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom"
  },
  "dependencies": {
    "@opuscapita/react-markdown": "^2.3.1",
    "antd": "^3.8.0",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-form": "^1.35.1",
    "mobx-react-router": "^4.0.4",
    "raven-js": "^3.26.3",
    "react": "^16.4.1",
    "react-debounce-input": "^3.2.0",
    "react-detect-offline": "^2.1.2",
    "react-dom": "^16.4.1",
    "react-ga": "^2.5.3",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^3.4.1",
    "react-modal": "^3.5.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "react-select": "^2.0.0",
    "react-styleguidist": "^7.2.5",
    "react-typography": "^0.16.13",
    "sha1": "^1.1.1",
    "styled-components": "^3.4.2",
    "typography": "^0.16.17",
    "validator": "^10.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "jest-fetch-mock": "^1.6.5",
    "jest-styled-components": "^5.0.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "lint-staged": "^7.2.0",
    "mobx-react-devtools": "^5.0.1",
    "prettier": "^1.14.2",
    "react-app-rewire-antd-theme": "^1.1.3",
    "react-app-rewire-less": "^2.1.2",
    "react-app-rewired": "^2.0.0",
    "react-testing-library": "^4.1.9",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,json,css,md}": [
      "prettier --write, git add"
    ]
  }
}
