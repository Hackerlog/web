{
  "name": "hackerlog-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://hackerlog.io",
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "analyze": "ANALYZE=1 react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "lodash-es": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-form": "^1.34.0",
    "mobx-state-tree": "^3.0.0",
    "mst-middlewares": "^3.0.0",
    "mst-react-router": "^2.0.0",
    "raven-js": "^3.26.3",
    "react": "^16.4.1",
    "react-debounce-input": "^3.2.0",
    "react-detect-offline": "^2.1.2",
    "react-dom": "^16.4.1",
    "react-ga": "^2.5.3",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.4.0",
    "react-modal": "^3.5.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-typography": "^0.16.13",
    "styled-components": "^3.3.3",
    "typography": "^0.16.17",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^10.5.2",
    "@types/raven-js": "^3.10.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-loadable": "^5.4.0",
    "@types/react-modal": "^3.2.0",
    "@types/react-router-dom": "^4.2.7",
    "@types/validatorjs": "^3.7.1",
    "husky": "^0.14.3",
    "jest-fetch-mock": "^1.6.5",
    "lint-staged": "^7.2.0",
    "mobx-react-devtools": "^5.0.1",
    "prettier": "^1.13.7",
    "react-app-rewired": "^1.5.2",
    "react-testing-library": "^4.1.2",
    "remotedev": "^0.2.7",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,json,css,md}": [
      "prettier --write, git add"
    ]
  }
}
