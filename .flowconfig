[ignore]
<PROJECT_ROOT>/.history/.*
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/public/.*
<PROJECT_ROOT>/src/services/api/.*

[include]
<PROJECT_ROOT>/src/.*

[libs]
